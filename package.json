{
  "name": "climateai-sdk",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "lex-api",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs"
    },
    "./utilities": {
      "types": "./dist/utilities/index.d.ts",
      "import": "./dist/utilities/index.js",
      "require": "./dist/utilities/index.cjs"
    },
    "./utilities/transformer": {
      "types": "./dist/utilities/transformer.d.ts",
      "import": "./dist/utilities/transformer.js",
      "require": "./dist/utilities/transformer.cjs"
    },
    "./utilities/hypercerts": {
      "types": "./dist/utilities/hypercerts.d.ts",
      "import": "./dist/utilities/hypercerts.js",
      "require": "./dist/utilities/hypercerts.cjs"
    },
    "./utilities/*": {
      "types": "./dist/utilities/*.d.ts",
      "import": "./dist/utilities/*.js",
      "require": "./dist/utilities/*.cjs"
    },
    "./zod-schemas": {
      "types": "./dist/zod-schemas/index.d.ts",
      "import": "./dist/zod-schemas/index.js",
      "require": "./dist/zod-schemas/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./session": {
      "types": "./dist/server/session.d.ts",
      "import": "./dist/server/session.js",
      "require": "./dist/server/session.cjs"
    },
    "./lex-api": {
      "types": "./dist/lex-api/index.d.ts",
      "import": "./dist/lex-api/index.js",
      "require": "./dist/lex-api/index.cjs"
    },
    "./lex-api/util": {
      "types": "./dist/lex-api/util.d.ts",
      "import": "./dist/lex-api/util.js",
      "require": "./dist/lex-api/util.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "utilities": [
        "dist/utilities/index.d.ts"
      ],
      "utilities/*": [
        "dist/utilities/*.d.ts"
      ],
      "lex-api": [
        "dist/lex-api/index.d.ts"
      ],
      "lex-api/*": [
        "dist/lex-api/*.d.ts"
      ]
    }
  },
  "private": true,
  "scripts": {
    "lexgen": "sh scripts/lexgen.sh",
    "build": "tsup --config tsup.config.ts"
  },
  "devDependencies": {
    "@atproto/lex-cli": "^0.9.7",
    "@types/bun": "latest",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@atproto/api": "0.18.8",
    "@atproto/oauth-client-node": "0.3.9",
    "@trpc/client": "^11.7.2",
    "@trpc/react-query": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "@turf/turf": "^7.3.0",
    "cid": "multiformats/cid",
    "geojson": "^0.5.0",
    "jose": "^6.1.2",
    "multiformats": "^13.4.1",
    "next": "^16.0.3",
    "superjson": "^2.2.5",
    "zod": "^4.1.13"
  }
}
