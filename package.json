{
  "name": "gainforest-sdk-nextjs",
  "version": "0.1.0",
  "description": "Gainforest SDK for Next.js - Type-safe tRPC SDK for ATProto PDS integration",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "lexgen": "sh scripts/lexgen.sh",
    "prebuild": "bun run scripts/prebuild.ts",
    "build": "tsup --config tsup.config.ts"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./lex-api": {
      "types": "./dist/lex-api.d.ts",
      "import": "./dist/lex-api.js",
      "require": "./dist/lex-api.cjs"
    },
    "./lex-api/utils": {
      "types": "./dist/lex-api/utils.d.ts",
      "import": "./dist/lex-api/utils.js",
      "require": "./dist/lex-api/utils.cjs"
    },
    "./lex-api/*": {
      "types": "./dist/lex-api/*.d.ts",
      "import": "./dist/lex-api/*.js",
      "require": "./dist/lex-api/*.cjs"
    },
    "./utilities": {
      "types": "./dist/utilities.d.ts",
      "import": "./dist/utilities.js",
      "require": "./dist/utilities.cjs"
    },
    "./utilities/atproto": {
      "types": "./dist/utilities/atproto.d.ts",
      "import": "./dist/utilities/atproto.js",
      "require": "./dist/utilities/atproto.cjs"
    },
    "./utilities/hypercerts": {
      "types": "./dist/utilities/hypercerts.d.ts",
      "import": "./dist/utilities/hypercerts.js",
      "require": "./dist/utilities/hypercerts.cjs"
    },
    "./utilities/transform": {
      "types": "./dist/utilities/transform.d.ts",
      "import": "./dist/utilities/transform.js",
      "require": "./dist/utilities/transform.cjs"
    },
    "./utilities/geojson": {
      "types": "./dist/utilities/geojson.d.ts",
      "import": "./dist/utilities/geojson.js",
      "require": "./dist/utilities/geojson.cjs"
    },
    "./utilities/*": {
      "types": "./dist/utilities/*.d.ts",
      "import": "./dist/utilities/*.js",
      "require": "./dist/utilities/*.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./session": {
      "types": "./dist/session.d.ts",
      "import": "./dist/session.js",
      "require": "./dist/session.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./zod": {
      "types": "./dist/zod.d.ts",
      "import": "./dist/zod.js",
      "require": "./dist/zod.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "utilities": [
        "dist/utilities.d.ts"
      ],
      "utilities/*": [
        "dist/utilities/*.d.ts"
      ],
      "lex-api": [
        "dist/lex-api.d.ts"
      ],
      "lex-api/*": [
        "dist/lex-api/*.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "private": true,
  "devDependencies": {
    "@atproto/lex-cli": "^0.9.7",
    "@changesets/cli": "^2.29.8",
    "@types/bun": "latest",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@atproto/api": "0.18.8",
    "@atproto/oauth-client-node": "0.3.9",
    "@trpc/client": "^11.7.2",
    "@trpc/react-query": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "@turf/turf": "^7.3.0",
    "cid": "multiformats/cid",
    "geojson": "^0.5.0",
    "jose": "^6.1.2",
    "multiformats": "^13.4.1",
    "next": "^16.0.3",
    "superjson": "^2.2.5",
    "zod": "^4.1.13"
  }
}
